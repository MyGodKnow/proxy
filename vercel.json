{
  "rewrites": [
    { 
      "source": "/:image(\d+_.+\\.webp)", 
      "destination": "https://www.wgzdy.top/wp-content/uploads/:image" 
    },
    { 
      "source": "/wp-content/uploads/:url*", 
      "destination": "https://www.wgzdy.top/wp-content/uploads/:url*" 
    }
  ],
  "headers": [
    {
      "source": "/(.*).(css|js|webp|jpg|png|gif|svg|ttf|eot|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=259200, immutable"
        }
      ]
    }
  ]
}
